channel(#AN_app_nuebar_CC1pi)     <@channel = #JHFminus: -: e:      e:    #CC1pi: #res_app_nuebar_CC1pi  @pre_smearing_efficiencies=copy(%eff_app_nuebar_CC1pi)>
channel(#AN_app_nuebar_NC)        <@channel = #JHFminus: -: NOSC_e: e:    #NC:    #res_app_nuebar_NC     @pre_smearing_efficiencies=copy(%eff_app_nuebar_NC)>
channel(#AN_app_numu_CCQE)        <@channel = #JHFminus: +: m:      m:    #CCQE:  #res_app_numu_CCQE     @pre_smearing_efficiencies=copy(%eff_app_numu_CCQE)>
//channel(#AN_app_numu_CC1pi)       <@channel = #JHFminus: +: m:      m:    #CC1pi: #res_app_numu_CC1pi    @pre_smearing_efficiencies=copy(%eff_app_numu_CC1pi)>
