channel(#AN_dis_numubar_NC)         < @channel = #JHFminus: -: NOSC_m: m: #NC:      #res_dis_numubar_NC      @pre_smearing_efficiencies=copy(%eff_dis_numubar_NC)>

/* ################################# */
/* ####### Setting the rules ####### */
/* ################################# */

errorfactor=0.75 //to account for correlated errors - custom treatment of systematics with correlations would be better

/* 2 */
rule(#AN_Appearance)<
    @signal = 1.072@#AN_app_nue_sig_CCQE :
              1.072@#AN_app_nue_sig_CC1pi :
              1.335@#AN_app_nuebar_sig_CCQE :
              1.335@#AN_app_nuebar_sig_CC1pi
    @background = 0.923@#AN_app_nue_CCQE :
                  0.923@#AN_app_nue_CC1pi :
                  0.542@#AN_app_nue_NC :
                  1.234@#AN_app_nuebar_CCQE :
