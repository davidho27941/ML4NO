%eff_dis_nuebar_CC1pi    = include "HK_globes/efficiencies/nuebar_eff_CC1pi_inFID_dis_new.glb.dat"
%eff_dis_nuebar_CCother  = include "HK_globes/efficiencies/nuebar_eff_CCother_inFID_dis_new.glb.dat"
