%eff_dis_nue_sig_CC1pi   = include "HK_globes/efficiencies/nue_sig_eff_CC1pi_inFID_dis_new.glb.dat"
%eff_dis_nue_sig_CCother = include "HK_globes/efficiencies/nue_sig_eff_CCother_inFID_dis_new.glb.dat"
