//cross(#CC1pi)   < @cross_file="HK_globes/xsec/H2Occ1pixsec.glb.dat"   >
//cross(#CCother) < @cross_file="HK_globes/xsec/H2Occotherxsec.glb.dat" >
//cross(#NC)      < @cross_file="HK_globes/xsec/H2Omostncxsec.glb.dat"      >

