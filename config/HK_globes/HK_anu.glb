channel(#AN_dis_nue_CC1pi)          < @channel = #JHFminus: +: e:      e: #CC1pi:   #res_dis_nue_CC1pi       @pre_smearing_efficiencies=copy(%eff_dis_nue_CC1pi)>
//channel(#AN_dis_nue_CCother)        < @channel = #JHFminus: +: e:      e: #CCother: #res_dis_nue_CCother     @pre_smearing_efficiencies=copy(%eff_dis_nue_CCother)>
channel(#AN_dis_nue_NC)             < @channel = #JHFminus: +: NOSC_e: e: #NC:      #res_dis_nue_NC          @pre_smearing_efficiencies=copy(%eff_dis_nue_NC)>
channel(#AN_dis_nuebar_CCQE)        < @channel = #JHFminus: -: e:      e: #CCQE:    #res_dis_nuebar_CCQE     @pre_smearing_efficiencies=copy(%eff_dis_nuebar_CCQE)>
